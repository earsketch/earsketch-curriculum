// Setup
init(); 
setTempo(80);

// Music
var startMeasure = 1;                   // Here, we assign number values
var endMeasure = startMeasure+2;        // to our variables.

fitMedia(HIPHOP_DUSTYGROOVE_001, 1, startMeasure, endMeasure); // Now we can use our variables instead of typing measure numbers
fitMedia(HIPHOP_DUSTYCOINLEAD_002, 2, startMeasure, endMeasure);

// Finish
finish();
