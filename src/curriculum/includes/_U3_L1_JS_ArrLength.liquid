//Setup
init();

//Using the Array's Length

var myArray = ["Get", "thee", "to", "the", "console!"];

for(var i = 0; i < myArray.length; i++) { // The stopping condition is determined by the length of the array we want to iterate through (in this case, it equals 5)
	println(myArray[i]);
}

//Finish
finish();
