//Setup
init();
setTempo(120);

//Music
fitMedia(Y33_CHOIR_1, 1, 1, 17);
fitMedia(RD_ELECTRO_DRUM_PART_10, 2, 1, 17);

for(var measure = 1; measure < 17; measure++) {
	setEffect(1, VOLUME, GAIN, -60, measure, 0, measure+1);
}

//Finish
finish();
