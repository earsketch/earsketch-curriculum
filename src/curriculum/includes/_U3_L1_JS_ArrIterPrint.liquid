//Setup
init();

//Array Iteration

var myArray = ["Get", "thee", "to", "the", "console!"];

for (var i = 0; i < 5; i++) {  // Since there are 5 items in our list, we know it uses indices 0-4. So we stop our loop before 5.
	println(myArray[i]);  // We use i as the index number
}

//Finish
finish();
