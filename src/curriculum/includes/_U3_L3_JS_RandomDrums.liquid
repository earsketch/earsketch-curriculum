init();
setTempo(170);

var drums = [OS_COWBELL01, OS_COWBELL02, OS_COWBELL03, OS_KICK01, OS_KICK02, OS_LOWTOM01];

var beatString = '';

for(var i = 0; i < 32; i++) {
	var index = Math.floor( Math.random() * 6 ); //Makes a random int between 0 and 6
	beatString = beatString + index;
}

makeBeat(drums, 1, 1, beatString);

finish();
